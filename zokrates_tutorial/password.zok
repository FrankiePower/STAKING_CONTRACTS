from "hashes/sha256/512bitPacked" import main as sha256packed;

def main(private field password, private field[2] guess) -> bool {
    field[2] secret = sha256packed([password, 0]);
    field[2] hashed_guess = sha256packed(guess);
    
    bool result = true;
    for u32 i in 0..2 {
        result = result && (secret[i] == hashed_guess[i]);
    }
    
    return result;
}